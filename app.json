        "backgroundColor": "#ffffff"
      },
      "permissions": ["CAMERA"],
      "package": "com.matheuschiodi.InstantQRCode",
      "versionCode": 4
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "30b5ce97-86b7-44c7-9dbd-12e9b656e350"
      }
    }
